<!--Calendar Modal-->

<div class="modal fade" id="modalFormCal">
  <div class="modal-dialog" role="document">
    <div class="modal-content">
      <div class="modal-header">
        <button type="button" class="close" data-dismiss="modal" aria-label="Close">
          <span aria-hidden="true">&times;</span>
        </button>
        <h4 class="modal-title">Add Calendar Entry</h4>
      </div>
      <div class="modal-body">
        
        <form id="formCal">
            <div class="form-group">
              <label for="txtCal">Event</label>
              <input type="text" id="txtCalendar" class="form-control">
              <small class="form-text text-muted" style="color: white;">This field is required</small>
              
            </div>

            <div class="form-group">
              <label for="when">Date</label>
              <small class="form-text text-muted" style="color: white;">This field is required</small>
              <div class="input-group" >
                  <input type="date" class="form-control" id="eventDate">
                  <div class="input-group-addon">
                      <span class="glyphicon glyphicon-th"></span>
                  </div>

                  
              </div>
            </div>

            <div class="form-group" id="fgEndDate">
              <label for="endDate">End Date</label>
              <small class="form-text text-muted" style="color: white;">This field is required</small>
              {{!-- <input id="endDate" name="endDate" type="date">               --}}
              <div class="input-group">
                  <input type="date" class="form-control" id="eventEndDate">
                  <div class="input-group-addon">
                      <span class="glyphicon glyphicon-th"></span>
                  </div>
                  
              </div>
            </div>

            <div class="form-group">
              <label for="qMultipleDays">Mutiple Days</label>
              <input type="checkbox" id="qMultipleDays">  
            </div>
        </form>

      </div>
      <div class="modal-footer">
        <button type="button" class="btn btn-secondary" data-dismiss="modal">Close</button>
        <button type="button" class="btn btn-primary" id="btnSubmitFormCal">Submit</button>
      </div>
    </div><!-- /.modal-content -->
  </div><!-- /.modal-dialog -->
